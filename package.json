  "author": "Matthew Harrison-Jones <matthojo@gmail.com>",
  "homepage": "http://coinolio.org",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
